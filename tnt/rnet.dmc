O BASE1 BASE2.
DEFINE INTEGER TDY.
SET FMSG OFF.
SET FERR OFF.
FI NET EQUAL MCIRNET.
PRINT TNUM " " STAT CNAM SVER CKT COMMNT MAINT ONUM ELM WHO.
PRINT TNUM DISC.
MAP TO 2 VIA TNUM.
EVAL TDY EQ (SYSDATE - 1).
SEARCH SDAT GE TDY.
PRINT TNUM SDAT ORDER UNAM WHO INFO.
CLOSE.
CLOSE.
QUIT.
