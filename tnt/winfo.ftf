C               SUBROUTINE FOR WRITING THE INFO TO DISK
C
        SUBROUTINE WINFO(INFO)
C
        INTEGER INFO(64),ILIN,N,J
C
        DO 50 ILIN=1,4
        N=(ILIN*16)-15
        IF (INFO(N).EQ.'.    ') GOTO 40
        WRITE(8,30)(INFO(J),J=N,N+15)
30      FORMAT(16A5)
        GOTO 50
40      ILIN=4
50      CONTINUE
        RETURN
        END
C
