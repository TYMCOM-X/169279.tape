DPL START.
DEFINE INTEGER STARTDATE ENDDATE REAL Y Z.
LET STARTDATE EQ 70004.
LET ENDDATE EQ 70034.
LOOP:
  FIND DUP STARTDATE AND SVER 1 AND HN LT 5 AND HN NE 2.
  SORT ELM ASCEN.
  LET Y EQ 0 Z EQ 0.
  GETR DONE 1.
  LET Y EQ ELM/60.
  GETR DONE SYSNREC.
  LET Z EQ ELM/60.
DONE:
  PRINT STARTDATE Y Z.
  LET STARTDATE EQ STARTDATE + 1.
  IF STARTDATE LE ENDDATE THEN LOOP.
DPL END.